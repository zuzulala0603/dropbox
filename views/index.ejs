<!--         <div onclick="removeUpload()" id="changeBtn" class="remove-image">사진 바꾸기</div> -->
      </div>
    </section>
    <input id="upload__input" class="file-upload-input" type="file" onchange="readURL(this);" accept="image/*" />
    <section id="footer"></section>
